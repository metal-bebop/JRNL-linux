{
  "homepage": "https://github.com/metalbebop/jrnl-linux",
  "name": "jrnl-linux",
  "version": "2.0.0",
  "description": "Journal",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "package-win32": "electron-packager . JRNL --overwrite --platform=win32 --arch=ia32 --icon=./icon.ico --prune=true --out=../releases",
    "package-osx": "electron-packager . JRNL --overwrite --platform=darwin --arch=x64 --icon=./icon.icns --prune=true --out=../releases",
    "installer-linux": "electron-builder --linux"  
  },
  "author": {
    "name": "AJ Bradley",
    "email": "abradley4646@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^16.0.7",
    "electron-builder": "^22.8.0",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "jquery": "^3.5.0"
  },
  "build": {
    "appId": "com.electron.jrnl",
    "productName": "JRNL",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "msi"
    },
    "linux": {
      "target": ["AppImage", "deb"],
      "icon": "jrnlicon.png",
      "category": "Utility",
      "maintainer": "AJ Bradley abradley4646@gmail.com"
  }
  }
}
